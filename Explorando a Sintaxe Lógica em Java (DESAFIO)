import java.util.*;

public class EstoqueLojas {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        String entrada = scanner.nextLine();
        System.out.println(calcularEstoque(entrada));
        scanner.close();
    }

    public static String calcularEstoque(String entrada) {
        Map<String, Integer> estoque = new LinkedHashMap<>();
        String[] itens = entrada.split(",");

        for (String item : itens) {
            String[] partes = item.split(":");
            String codigo = partes[0];
            int quantidade = Integer.parseInt(partes[1]);
            estoque.put(codigo, estoque.getOrDefault(codigo, 0) + quantidade);
        }

        StringBuilder resultado = new StringBuilder();
        for (Map.Entry<String, Integer> entry : estoque.entrySet()) {
            if (resultado.length() > 0) {
                resultado.append(",");
            }
            resultado.append(entry.getKey()).append(":").append(entry.getValue());
        }

        return resultado.toString();
    }

}
